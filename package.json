{
  "name": "environment",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "node server/_server",
    "dev:phone": "parcel watch phone/index.html --dist-dir public/phone --no-hmr",
    "dev:projector": "parcel watch projector/index.html --dist-dir public/projector --no-hmr --public-url /projector",
    "dev:review": "parcel watch review/index.html --dist-dir public/review --no-hmr --public-url /review",
    "dev:screenshots": "parcel watch screenshots/index.html --dist-dir public/screenshots --no-hmr --public-url /screenshots",
    "dev": "npm-run-all --parallel dev:*",
    "build:phone": "rimraf public/phone && parcel build phone/index.html --dist-dir public/phone --no-source-maps",
    "build:projector": "rimraf public/projector && parcel build projector/index.html --dist-dir public/projector --public-url /projector --no-source-maps",
    "build:review": "rimraf public/review && parcel build review/index.html --dist-dir public/review --public-url /review --no-source-maps",
    "build:screenshots": "rimraf public/screenshots && parcel build screenshots/index.html --dist-dir public/screenshots --public-url /screenshots --no-source-maps",
    "build": "npm-run-all build:*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime-corejs2": "^7.17.2",
    "@tweenjs/tween.js": "^18.6.4",
    "babel-polyfill": "^6.26.0",
    "basic-auth": "^2.0.1",
    "blueimp-load-image": "^5.16.0",
    "dat.gui": "^0.7.9",
    "dotenv": "^16.0.0",
    "dotenv-expand": "^8.0.1",
    "express": "^4.17.3",
    "lodash": "^4.17.21",
    "lodash.random": "^3.2.0",
    "nanohtml": "^1.9.1",
    "npm-run-all": "^4.1.5",
    "pixelate": "0.0.5",
    "projector": "^3.1.0",
    "simplex-noise": "^3.0.1",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@parcel/transformer-image": "^2.3.2",
    "@parcel/transformer-sass": "^2.3.2",
    "parcel": "^2.3.2",
    "rimraf": "^3.0.2",
    "sass": "^1.49.9"
  }
}
